{
  "name": "Query Service API",
  "api_id": "query-service-api",
  "org_id": "1",
  "active": true,
  "use_keyless": false,
  "auth": {
    "auth_header_name": "Authorization"
  },
  "global_rate_limit": {
    "rate": 100,
    "per": 60
  },
  "cors": {
    "enable": true,
    "allowed_origins": [
      "http://localhost:5173"
    ],
    "allowed_methods": [
      "GET"
    ],
    "allowed_headers": [
      "Authorization"
    ],
    "allow_credentials": true,
    "options_passthrough": false
  },
  "proxy": {
    "listen_path": "/api/",
    "target_url": "http://query-service:8082/",
    "strip_listen_path": true
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "use_extended_paths": true
      }
    }
  }
}
